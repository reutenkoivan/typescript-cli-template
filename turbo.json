{
  "$schema": "https://turborepo.com/schema.json",
  "ui": "tui",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**"]
    },
    "dev": {},
    "doc:start": {},
    "doc:build": {},
    "topo": {
      "dependsOn": ["^topo"]
    },
    "lint:types": {
      "dependsOn": ["topo"]
    },
    "transit": {
      "dependsOn": ["^transit"]
    },
    "test": {
      "dependsOn": ["^test"]
    },
    "test:watch": {
      "cache": false,
      "persistent": true
    },
    "test:ui": {
      "cache": false,
      "persistent": true
    }
  }
}
